    "size-limit": "^11.1.6",
    "start-server-and-test": "^2.0.10",
    "stylelint": "^16.14.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "ts-node": "^10.9.2",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "vitest-fetch-mock": "^0.4.3",
    "yargs": "^17.7.2"
  },
  "scripts": {
    "start": "APP_ENV=development vite",
    "build": "tsc && APP_ENV=production vite build",
    "lint": "eslint --fix src test",
    "lint:styles": "stylelint --report-needless-disables 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "format": "prettier \"**/*.{ts,tsx}\" --write",
    "validate": "npm run lint && npm run lint:styles && npm run typecheck &&  npm run test:coverage && npm run build && npm run size",
    "serve": "serve --listen 3000 dist",
    "size": "size-limit",
    "prepare": "husky",
    "deploy": "node tools deploy",
    "publish": "node tools publish"
  },
  "eslintConfig": {
    "extends": [
      "@gilbarbara/eslint-config"
    ],
    "plugins": [
      "@emotion"
    ],
    "overrides": [
      {
        "files": [
          "test/**/*.ts?(x)"
        ],
        "rules": {
          "no-console": "off"
        }
      }
    ]
  },
  "prettier": "@gilbarbara/prettier-config",
  "size-limit": [
    {
      "path": "./dist/assets/*.js",
      "limit": "200 kb"
    }
  ],
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --fix",
      "stylelint --fix",
      "git add",
      "vitest --findRelatedTests"
    ],
    "*.(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
