  "scripts": {
    "start": "APP_ENV=development node tools/start",
    "build": "APP_ENV=production node tools/build",
    "analyze": "NODE_ENV=production webpack --config config/webpack.config.js --profile --json > webpack.stats.json",
    "lint": "eslint  --ext .js --ext .jsx app/scripts config test",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --bail --coverage",
    "test:watch": "jest --watch --verbose",
    "format": "prettier \"**/*.{js,jsx}\" --write",
    "validate": "npm run lint && npm run lint:styles && npm run test:coverage && npm run bundlesize",
    "bundlesize": "bundlesize",
    "serve": "serve --listen 3000 build",
    "deploy": "node tools deploy",
    "publish": "node tools publish"
  },
  "browserslist": [
    "last 2 versions",
    "safari >= 8",
    "ios > 8"
  ],
  "bundlesize": [
    {
      "path": "./build/scripts/bundle.*.js",
      "maxSize": "170 kB"
    },
    {
      "path": "./build/scripts/modernizr.*.js",
      "maxSize": "30 kB"
    }
  ],
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "jsxBracketSameLine": false
  },
  "lint-staged": {
    "*.js?(x)": [
      "eslint --fix",
      "git add",
      "jest --findRelatedTests"
    ],
    "*.(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-merge": "node tools update",
      "pre-commit": "node tools upstream && lint-staged && flow"
    }
  }
}
